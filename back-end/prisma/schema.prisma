  altura      Float?
  comestiveis String?   @db.VarChar(2048)
  tangiveis   String?   @db.VarChar(2048)
  fisicos     String?   @db.VarChar(2048)
  data        DateTime  @default(now())
  data_neuro  DateTime? @db.Date
  alergicos   String?
  laudos      String[]  @db.VarChar(2048)

  @@ignore
}

